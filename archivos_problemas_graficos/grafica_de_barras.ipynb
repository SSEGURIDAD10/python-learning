    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "df = pd.read_csv(\"ingresos.csv\")\n",
    "\n",
    "sns.barplot(x=\"Fuente\",y=\"Ingresos\",data=df)\n",
    "\n",
    "total_ingresos = df['Ingresos'].sum()\n",
    "\n",
    "print(f'El total de ingresos es: ${total_ingresos} USD')\n",
    "\n",
    "plt.show()"
   ]
  }
